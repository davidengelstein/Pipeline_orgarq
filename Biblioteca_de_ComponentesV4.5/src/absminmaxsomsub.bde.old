"max <= a \t\twhen (a >= b) else -- Calcula valor Máximo\n"+
"\t\t\t\t b;\n"+
"absa <= a \t\twhen (a(NumeroBits - 1) = '0') else -- Calcula valor absoluto\n"+
"\t\t\t\t\t(not a + 1);\n"+
"absb <= b\t\twhen (b(NumeroBits - 1) = '0') else -- Calcula valor absoluto\n"+
"\t\t\t\t\t(not b + 1);\n"+
"unidade_funcional:\n"+
"with C select\n"+
"\tf <=\t(a + b)\tafter Tsom \twhen \"001\", -- Executa SOMA\n"+
"\t\t\t\tabsb\t\tafter Tabs\t when \"010\", -- Executa Valor absoluto b\n"+
"\t\t\t\t(b - a)\t after Tsub\t when \"011\", -- Executa subtração\n"+
"   \t\t\tabsa\t\tafter Tabs \twhen \"100\",  -- Executa Valor absoluto a\n"+
"\t\t\t\t(a - b)\t after Tsub \twhen \"101\", -- Executa Subtração\n"+
"\t\t\t\tmin\t\t  after Tmm \twhen \"110\", -- Executa Mínimo\n"+
"\t\t\t\tmax\t\t after Tmm \twhen \"111\" , -- Executa Máximo\n"+
"\t\t\t\t(others => '0') \t\t\t  when others;\n"+
"\n"+
""
